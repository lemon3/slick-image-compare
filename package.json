{
  "name": "slick-image-compare",
  "version": "0.4.3",
  "description": "before and after image comparison slider",
  "author": "wolfgang jungmayer",
  "license": "MIT",
  "browser": "dist/slick-image-compare.umd.js",
  "type": "module",
  "main": "dist/slick-image-compare.umd.js",
  "module": "./dist/slick-image-compare.js",
  "exports": {
    ".": {
      "import": "./dist/slick-image-compare.js",
      "require": "./dist/slick-image-compare.umd.js"
    },
    "./dist/*.css": "./dist/*.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs": "vite build --config vite.config.pages.js",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.js'",
    "format": "prettier --write 'src/**/*.js'",
    "test": "vitest",
    "bench": "vitest bench",
    "coverage": "vitest --coverage",
    "browserslistupdate": "npx update-browserslist-db@latest"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "index.js"
  ],
  "keywords": [
    "slick-image-compare",
    "beforeafter",
    "before-after",
    "before-after-slider",
    "images",
    "slider",
    "compare",
    "compare-slider",
    "compare-viewer",
    "comparison",
    "comparison-slider",
    "comparison-viewer",
    "image-compare",
    "image-compare-viewer",
    "image-compare-slider",
    "image-comparison",
    "image-comparison-slider",
    "image-comparison-viewer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon3/slick-image-compare.git"
  },
  "homepage": "https://lemon3.github.io/slick-image-compare",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-properties": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "postcss-nested": "^6.2.0",
    "postcss-preset-env": "^10.0.5",
    "prettier": "^3.3.3",
    "vite": "^5.4.8",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-banner": "^0.8.0",
    "vitest": "^2.1.1"
  }
}
